function Answer = dialog_default_saver
% Construct a questdlg with three options
choice = questdlg('This save path will be saved for further sessions. Do you want to overwrite the previous default settigns?', ...
	'TrierAnalysis', ...
	'Yes','No','Yes');
% Handle response
switch choice
    case 'Yes'
        Answer = 1;
    case 'No'
        Answer = 0;
end